{
  "name": "kiwi-bundle",
  "version": "2.3.0",
  "description": "A simple and clean framework for creating cross-platform applications",
  "license": "Apache-2.0",
  "author": "Nazim Lachter <nazim@blueforest.fr>",
  "repository": "git@github.com:theblueforest/kiwi-bundle.git",
  "main": "./lib/index.js",
  "bin": {
    "kiwi": "./lib/cli/index.js"
  },
  "scripts": {
    "start": "tsc -w -p .",
    "build": "rm -rf lib/ && tsc -p . && chmod +x lib/cli/*.js",
    "cli": "./lib/cli/index.js",
    "test": "echo SKIPPED",
    "preversion": "git pull && (test $(git rev-parse --abbrev-ref HEAD) != master && echo '/!\\ YOU MUST CHECKOUT ON MASTER' && exit 1 || npm test) && npm run build && git add --all",
    "version:major": "npm version major -m 'Major upgrade from ${npm_package_version} to %s'",
    "version:minor": "npm version minor -m 'Minor upgrade from ${npm_package_version} to %s'",
    "version:patch": "npm version patch -m 'Patch upgrade from ${npm_package_version} to %s'",
    "postversion": "git push --follow-tags && echo '=> You can now run `npm publish`'"
  },
