{
  "ignorePatterns": ["node_modules/", "android/", "ios/"],
  "rules": {
    "array-bracket-spacing": [
      2,
      "always",
      {
        "objectsInArrays": false
      }
    ],
    "quotes": [2, "double"],
    "comma-dangle": [2, "always-multiline"],
    "semi": [2, "never"],
    "object-curly-spacing": [2, "always"],
    "array-bracket-newline": [2, "always"],
    "space-after-keywords": [2, "never"]
  }
}